[loggers]
keys=root

[formatters]
keys=fdef

[handlers]
keys=hdef

[formatter_fdef]
format=%(asctime)s	%(name)s	%(levelname)s	%(message)s
datefmt=%Y-%m-%d %H:%M:%S
class=logging.Formatter

[handler_hdef]
class=logging.handlers.TimedRotatingFileHandler
level=INFO
formatter=fdef
args=('idxd.log','midnight',1)

[logger_root]
level=INFO
handlers=hdef

